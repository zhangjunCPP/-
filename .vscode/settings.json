    "luogu.defaultWaitingTime": 1000,
    "cph.general.timeOut": 100000,
    "cph.general.useShortCodeForcesName": true,
    "cph.language.cpp.SubmissionCompiler": "GNU G++14 6.4.0",
    "cph.language.cpp.Args": "-std=c++14 -O2 -Wall -Wextra",
    "cph.general.menuChoices": "cpp",
    "dateTime.customFormat": "YYYY-MM-DD dddd A HH:mm:ss",
    "dateTime.statusBarPriority": 1000,
    "luogu.effectiveDuration": -1,
    "vscode-office.editorLanguage": "zh_CN",
    "editor.fontLigatures": true, // 是否启用字体连字
    "editor.fontSize": 15, //设置字体大小
    "editor.fontWeight": "normal", //设置字体粗细
    "editor.fontFamily": "'JetBrains Mono'",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "name": "Comment",
                "scope": [
                    "comment",
                    "comment.block",
                    "comment.block.documentation",
                    "comment.line",
                    "comment.line.double-slash",
                    "punctuation.definition.comment",
                ],
                "settings": {
                    "foreground": "#1b66b1", //设置颜色
                    "fontStyle": "italic", //斜体 
                }
            },
        ]
    },
    "cph.general.defaultLanguage": "cpp",
    "editor.mouseWheelZoom": true,
    "files.autoSave": "onFocusChange",
    "editor.detectIndentation": true,
    "editor.insertSpaces": false
}